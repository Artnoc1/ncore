#include <cxxtest/TestSuite.h>
#include <Pins.h>

const int LOW = 0;
const int HIGH = 1;

class PinsTestSuite: public CxxTest::TestSuite
{
  Pins pins;
public:
  void setUp()
  {
  }

  void testEmpty( void )
  {
    int value = pins.digitalRead(0);

    TS_ASSERT_EQUALS( value, LOW );
  }
};
